  "channels": [
    "#channelstojoin"
  ],
  "log": {
    "severity": -1000
  },
  "plugins": {
    "weather": {
      "apikey": "fill in",
      "defaultLocation": "Oulu"
    },
    "youtube": {
      "apikey": "fill in",
      "bannedUppers": [
        "list of strings of uploaders",
        "who can't avoid the wrath of nobot"
      ]
    },
    "wolframalpha": {
      "apikey": "fill in"
    }
  }
}
